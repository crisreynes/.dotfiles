# .bash_aliases

alias ls="exa -a --icons --group-directories-first --color=always"
alias ll="exa -la --icons --group-directories-first --color=always"
alias cp="cp -iv"
alias mv="mv -iv"
alias rm="rm -vI"
alias cat="bat"
alias cal="cal --sunday"
alias za="zathura --fork"
alias v="nvim"
alias sv="sudo nvim"
alias lf="lfcd"
alias nb="newsboat"
alias :q="exit"
